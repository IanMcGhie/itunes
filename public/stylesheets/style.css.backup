@font-face {
    font-family: "Hack-Regular";
    font-size: 12px;
    src: url("/fonts/Hack-Regular.ttf");
    font-family: "DS-Regular";
    src: url("/fonts/digital.ttf");
}

body {
    color: #0d0;
    background: url(/images/space.png);
    text-align: center;
    font-family: "Hack-Regular";

}

option {
    background: #000;
    color: #0d0;
}

strong {
    color: #fede18;
}

#songtitle {
    font-size: large;
    height: 75px;
}

#winamp {}

#timeremaining {
    position: relative;
    bottom: 170px;
    right: 118px;
    font-family: "DS-Regular";
    font-size: 40px;
}

#volume {
    display: inline-block;
    background-color: #d37824;
    height: 3px;
    width: 95px;
    bottom: 158px;
    left: 50px;
}

#progressbar {
    position: relative;
    bottom: 128px;
}

#shuffleenabled {
    position: relative;
    bottom: 103px;
    right: 31px;
}

.ui-slider .ui-slider-handle {
    background: url(/images/volumeknob.png);
    background-size: cover;
    height: 13px;
    width: 20px;
    right: 0px;
    top: -7px;
}

#dialog {
    text-align: center;
    position: fixed;
    width: 50%;
    margin-left: -25%;
    padding: 20px 0px 20px 0px;
    top: 40%;
    left: 50%;
    background: #444;
    border: 1px solid #0f0;
    display: none;
}

.group {
    text-align: center;
}

#autocompleteform {
    display: inline;
}

#autocompleterow {
    text-align: center;
    font: inherit;
}

#playlist {
    font: inherit;
    margin: auto;
}

#mp3search {
    margin: 25px 0px 25px 0px;
    font: inherit;
    text-align: left;
    border: 1px solid #0f0;
    background-color: #000;
    color: inherit;
    width: 70%;
}

#playsong,
#queuesong {
    font: inherit;
    margin: 5px 5px 5px 5px;
    border: 1px solid #0f0;
    background-color: #000;
    color: #0f0;
}